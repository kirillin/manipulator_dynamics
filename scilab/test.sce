clear;
Q1 = [0.1:0.1:300, 300:-0.2:0.2, 0.2:0.02:10, 10:0.1:400, 400:-1:293];
Q2 = [0:0.1:899.9];
Q3 = [exp(-Q2/100+4.5)];
Q4 = [sin(-Q2)];
Q5 = [0.1:0.1:100, 100:-0.1:0.1, 0.1:0.1:100, 100:-0.1:0.1,0.1:0.1:100, 100:-0.1:0.1,0.1:0.1:100, 100:-0.1:0.1,0.1:0.1:100];

bigX = zeros(5*9000, 15);
    
for i = 1:9000
    q1 = Q1(i);
    q2 = Q2(i);
    q3 = Q3(i);
    q4 = Q4(i);
    q5 = Q5(i);
    
    // q3 != 0; q5 != 0
    X = [
        42*q1^7,  -5*q3,  q4*q2,    0,       1,  sin(q4), exp(-q1), 0, cos(q5 - 2*q4^2), 10*q4^4, 1 / q5, q3^5, sqrt(q5*q3^2), 0, q1*q4^2;
        0,      0,  0, -q2, tan(q3), sqrt(q5*q2^8),      0, 8*sqrt(abs(q5)), q1*q2*q3*q4, q1*q5/q3, q3/q5+1, sin(q5*q1^2), sin(2*q4), 1, 0;
        zeros(1,6), q3*q1^10, 3*sin(7*q4), -sqrt(q3), 0, q4*q5, q5*%pi, %pi, 2*cos(3*q4) + sin(q2), tan(3*q2)^2;
        zeros(1,9), 0, 1, exp(-q3^2), %e*%pi-q5, q4, sqrt(q5);
        zeros(1,12), abs(q2) * q1^2, - q3^2 * q4^3, q5*q1^2 
    ]
    
    bigX((i-1)*5+1:(i-1)*5+5, :) = X
end


K = [1,-0.5, 42, %pi, -2, 29, 15, -3, 17, 0.1, -0.707, sqrt(2), -0.003, 1.99, 80.14]';
Y = bigX * K;



fprintfMat('~/bigX.txt', bigX);
fprintfMat('~/Y.txt', Y);

